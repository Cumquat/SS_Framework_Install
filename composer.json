{
	"name": "cumquat/ss_framework_install",
	"description": "SilverStripe Framework Installer",
	"require": {
		"php": ">=5.5.0",
		"silverstripe/framework": "3.4.1",
		"silverstripe/siteconfig": "3.4.1",
		"silverstripe-themes/simple": "3.4.1"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"master": "master"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}

